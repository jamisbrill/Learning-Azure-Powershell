{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "ukwest"
        },

        "networkSecurityGroupName": {
            "value": "Mc-nsg"
        },
        "networkSecurityGroupRules": {
            "value": [
                {
                    "name": "SSH",
                    "properties": {
                        "priority": 300,
                        "protocol": "TCP",
                        "access": "Allow",
                        "direction": "Inbound",
                        "sourceAddressPrefix": "*",
                        "sourcePortRange": "*",
                        "destinationAddressPrefix": "*",
                        "destinationPortRange": "22"
                    }
                }
            ]
        },
        "subnetName": {
            "value": "default"
        },
        "virtualNetworkId": {
            "value": "/subscriptions//resourceGroups/Mc/providers/Microsoft.Network/virtualNetworks/Mc-vnet"
        },
        "publicIpAddressName": {
            "value": "Mc-ip"
        },
        "publicIpAddressType": {
            "value": "Dynamic"
        },
        "publicIpAddressSku": {
            "value": "Basic"
        },
        "virtualMachineName": {
            "value": "Mc"
        },
        "virtualMachineComputerName": {
            "value": "Mc"
        },
        "virtualMachineRG": {
            "value": "Mc"
        },
        "osDiskType": {
            "value": "Standard_LRS"
        },
        "virtualMachineSize": {
            "value": "Standard_B1s"
        },
        "adminUsername": {
            "value": "jam"
        },
     
    }
}
